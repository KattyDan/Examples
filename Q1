#1
class Car:
    def __init__(self, make, model, year): #атрибуты
        self.make = make
        self.model = model
        self.year = year

    def start_engine(self): #метод
        print(f"{self.make} {self.model} engine started")

    def stop_engine(self): #метод
        print(f"{self.make} {self.model} engine stopped")
